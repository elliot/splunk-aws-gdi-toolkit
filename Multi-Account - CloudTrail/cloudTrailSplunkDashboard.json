		"viz_NY0AedMk": {
			"type": "splunk.line",
			"title": "Event Latency over Time",
			"showProgressBar": false,
			"showLastUpdated": false,
			"dataSources": {
				"primary": "ds_srz6OcK5"
			},
			"options": {
				"legendDisplay": "off"
			}
		},
		"viz_XqNxe7aK": {
			"type": "splunk.singlevalue",
			"title": "Latest Event Latency",
			"dataSources": {
				"primary": "ds_tCds3VFx"
			},
			"options": {
				"sparklineDisplay": "off",
				"unit": "seconds"
			},
			"showProgressBar": false,
			"showLastUpdated": false
		}
	},
	"dataSources": {
		"ds_6WBt9iY9": {
			"type": "ds.search",
			"options": {
				"query": "index=_internal source=*license_usage* st=aws:cloudtrail NOT source=*license_usage_summary.log*\n| timechart sum(b) as ingestedDataGB\n| eval ingestedDataGB = ingestedDataGB/1024/1024/1024",
				"queryParameters": {
					"earliest": "$global_time.earliest$",
					"latest": "$global_time.latest$"
				}
			},
			"name": "licenseUsageBase"
		},
		"ds_HUEXsOZi": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_6WBt9iY9",
				"query": "|  stats sum(ingestedDataGB) as ingestedDataGB"
			},
			"name": "licenseUsageTotal"
		},
		"ds_7YVGDkt0": {
			"type": "ds.search",
			"options": {
				"query": "index=_internal series=\"aws:cloudtrail\" sourcetype=splunkd source=*metrics.log*\n| timechart sum(ev) as eventCount"
			},
			"name": "eventCountBase"
		},
		"ds_LN5KxPiz": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_7YVGDkt0",
				"query": "|  stats sum(eventCount) as eventCount"
			},
			"name": "eventCountTotal"
		},
		"ds_FFly4UzK": {
			"type": "ds.search",
			"options": {
				"query": "`cloudTrailIndex` sourcetype=aws:cloudtrail\n| timechart dc(recipientAccountId) as accountCount"
			},
			"name": "accountCountTimechart"
		},
		"ds_noJxIRWy": {
			"type": "ds.search",
			"options": {
				"query": "`cloudTrailIndex` sourcetype=aws:cloudtrail\n| stats dc(recipientAccountId) as accountCount"
			},
			"name": "accountCountTotal"
		},
		"ds_srz6OcK5": {
			"type": "ds.search",
			"options": {
				"query": "`cloudTrailIndex` sourcetype=aws:cloudtrail \n| eval eventLatency = abs(_indextime - _time)\n| timechart avg(eventLatency) as eventLatency"
			},
			"name": "eventLatencyBase"
		},
		"ds_tCds3VFx": {
			"type": "ds.search",
			"options": {
				"query": "`cloudTrailIndex` sourcetype=aws:cloudtrail | head 1\n| eval eventLatency = abs(_indextime - _time)\n| table eventLatency"
			},
			"name": "eventLatencyLatest"
		}
	},
	"defaults": {
		"dataSources": {
			"ds.search": {
				"options": {
					"queryParameters": {
						"latest": "$global_time.latest$",
						"earliest": "$global_time.earliest$"
					}
				}
			}
		}
	},
	"inputs": {
		"input_global_trp": {
			"type": "input.timerange",
			"options": {
				"token": "global_time",
				"defaultValue": "-24h@h,now"
